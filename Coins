package model;

public class Coins {
    private String name;
    private int value;
    
    public Coins(String name, int value) {
        this.name = name;
        this.value = value;
    }
    
    public String getName() {
        return name;
    }
    
    public void setName(String name) {
        this.name = name;
    }
    
    public int getValue() {
        return value;
    }
    
    public void setValue(int value) {
        this.value = value;
    }
    
    public void print() {
        System.out.println("Coin name: " + name);
        System.out.println("Coin value: " + value);
    }
}

